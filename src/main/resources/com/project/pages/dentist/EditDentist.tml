<html t:type="DentistLayout" title="Edit profile"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
        xmlns:p="tapestry:parameter">

        <!--todo password hide-->
        <t:beaneditform t:id="editForm" submitlabel="Edit Account" object="dentist"
                       reorder="firstName,lastName,address,emailAddress,telephoneNumber,birthDate">
                <p:password>
                        <t:label for="password"/>
                        <t:passwordfield t:id="password" value="dentist.password"/>
                </p:password>
        <p:id>
                <t:hidden value="dentist.id"/>
        </p:id>
        </t:beaneditform>

</html>